{
  "name": "market-csgo-api",
  "version": "0.5.0",
  "description": "A module for interacting with the market.csgo.com API. Release be like 1.0.0",
  "modules": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/malsatin/market-csgo-api.git"
  },
  "keywords": [
    "node",
    "csgo",
    "csgotm",
    "csgo.tm",
    "market",
    "weapons",
    "market.csgo",
    "api",
    "sdk"
  ],
  "scripts": {
    "build": "tsc  -p  ./src",
    "test": "NODE_ENV=test jest test/index.test.ts  --runInBand --detectOpenHandles --testTimeout=10000",
    "dev": "nodemon dist/index.js --exec  jest test/index.test.ts  --runInBand --detectOpenHandles --testTimeout=10000"
  },
  "publishConfig": {
    "access": "public"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules"
    ],
    "delay": 2500
  },
  "bugs": {
    "url": "https://github.com/malsatin/market-csgo-api/issues"
  },
  "homepage": "https://github.com/malsatin/market-csgo-api#readme",
  "author": "Malsa, mishanya",
  "license": "GNU GPLv3",
  "_moduleAliases": {
    "@validateWithJoi": "helpers/validators/index",
    "@emitters": "emitters/index",
    "@ErrorEmitter": "emitters/error",
    "@defaultValidationSchema": "enums/validation_schemas/default_validation_schema"
  },
  "jest": {
    "coverageDirectory": "test/coverage",
    "setupFiles": [
      "./test/setup.ts"
    ],
    "testMatch": [
      "**/test/index.test.ts"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@defaultValidationSchema": "<rootDir>/enums/validation_schemas/default_validation_schema",
      "@ErrorEmitter": "<rootDir>/emitters/error",
      "@validateWithJoi": "<rootDir>/helpers/validators/index",
      "@emitters": "<rootDir>/emitters/index"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-typescript": "^7.13.0",
    "bottleneck": "^2.19.5",
    "config": "^3.3.6",
    "fs": "0.0.1-security",
    "got": "^11.8.2",
    "joi": "^17.4.0",
    "merge": "^2.1.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.7",
    "papaparse": "^5.3.0",
    "source-map-loader": "^2.0.1",
    "ts-loader": "^9.1.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "babel-plugin-add-module-exports": "^1.0.4",
    "jest": "^26.6.3",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  }
}
