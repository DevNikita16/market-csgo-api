{
  "presets": [
    [
      '@babel/preset-env',
      {
        targets: {
          node: true
        }
      }
    ]
  ],
  "plugins": [
    '@babel/plugin-proposal-class-properties',
    "add-module-exports",
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ]
}

